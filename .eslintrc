{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-path-concat": 0,
        "one-var": 0,
        "padded-blocks": 0
    },
    "env": {
        "node": true
    },
    "extends": "standard"
}
